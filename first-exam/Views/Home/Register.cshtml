@{
    Layout = "_LoginRegisterLayout";
}


<div class="center-card">
                <div class="text-box">
                    <h1 class="heading h3">Get Started</h1>
                    <p class="paragraph medium">
                        Ut enim ad minim veniam, quis nostrud exercitation ullamco.
                    </p>
                </div>
                <div class="spacer _16"></div>
                <div class="form-wrapper w-form">
                    <form
                        id="wf-form-Get-Started-Form"
                        name="wf-form-Get-Started-Form"
                        data-name="Get Started Form"
                        method="get"
                        class="form"
                        data-wf-page-id="650b1180815c14a2cd40f686"
                        data-wf-element-id="2551c1ae-ffa7-ae0a-1080-51332b8ac0fa">
                        <input
                            class="text-input w-input"
                            maxlength="256"
                            name="Name-2"
                            data-name="Name 2"
                            placeholder="Enter your name"
                            type="email"
                            id="Name-2" /><input
                            class="text-input no-margin w-input"
                            maxlength="256"
                            name="Email-2"
                            data-name="Email 2"
                            placeholder="Enter your email"
                            type="email"
                            id="Email-2"
                            required="" />
                        <div class="spacer _24"></div>
                        <input
                            type="submit"
                            data-wait="Please wait..."
                            class="button no-margin w-button"
                            value="Sign Up" />
                    </form>
                    <div class="form-success w-form-done">
                        <div>Thank you! Check your email for confirmation.</div>
                    </div>
                    <div class="form-error w-form-fail">
                        <div>Oops! Something went wrong while submitting the form.</div>
                    </div>
                </div>
                <div class="spacer _16"></div>
                <p class="paragraph small no-margin">
                    By clicking the button above, you agree to our
                    <a href="/terms-conditions" target="_blank">Terms &amp; Conditions</a> and
                    <a href="/privacy-policy" target="_blank">Privacy Policy</a>.
                </p>
            </div>